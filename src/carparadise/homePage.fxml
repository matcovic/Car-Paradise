                     </children>
                  </VBox>
               </content>
               <font>
                  <Font size="14.0" />
               </font>
            </TitledPane>
            <Label fx:id="welcomeLabel" alignment="CENTER" layoutX="8.0" prefHeight="46.0" prefWidth="193.0" textFill="WHITE" wrapText="true">
               <font>
                  <Font name="Berlin Sans FB Demi Bold" size="22.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <AnchorPane prefHeight="77.0" prefWidth="923.0" style="-fx-background-color: #333333;">
         <children>
            <ImageView fitHeight="70.0" fitWidth="94.0" layoutX="8.0" layoutY="4.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/cpLogo2.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="46.0" fitWidth="124.0" layoutX="89.0" layoutY="38.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/car.png" />
               </image>
            </ImageView>
            <Label layoutX="384.0" layoutY="17.0" text="Car Paradise" textFill="#dd4f42">
