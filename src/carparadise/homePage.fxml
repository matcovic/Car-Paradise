<AnchorPane id="AnchorPane" prefHeight="736.0" prefWidth="1138.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="carparadise.HomePageController">
    <stylesheets>
        <URL value="@homepage.css" />
    </stylesheets>
   <children>
      <AnchorPane cache="true" layoutX="923.0" prefHeight="736.0" prefWidth="215.0" style="-fx-background-color: #333333;">
