                  <TextField fx:id="lowerBoundPrice" layoutX="14.0" layoutY="164.0" prefHeight="26.0" prefWidth="170.0" promptText="From" style="-fx-background-color: #ffff;" />
                  <TextField fx:id="upperBoundPrice" layoutX="14.0" layoutY="206.0" prefHeight="26.0" prefWidth="170.0" promptText="To" style="-fx-background-color: #ffff;" />
