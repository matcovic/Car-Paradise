          <TableColumn fx:id="idCol" prefWidth="61.0" text="ID" />
            <TableColumn fx:id="pictureCol" prefWidth="243.0" text="Picture" />
          <TableColumn fx:id="brandCol" prefWidth="191.0" text="Brand" />
            <TableColumn fx:id="ModelCol" prefWidth="200.0" text="Model" />
            <TableColumn fx:id="priceCol" prefWidth="209.0" text="Price" />
        </columns>
      </TableView>
      <Label fx:id="errorMessage" layoutX="240.0" layoutY="654.0" prefHeight="18.0" prefWidth="193.0" />
